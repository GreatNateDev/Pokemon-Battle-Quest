func savefile(Player,money,starter,mon1,mon2,mon3,mon4,mon5,mon6,index):
	DirAccess.make_dir_absolute("user://Saves")
	var file = FileAccess.open("user://Saves/Pokemon.save", FileAccess.WRITE)
	if file == null:
		var err_str := error_string(FileAccess.get_open_error())
		push_warning("Failed to open save file because: ", err_str)
		return
	starter = JSON.stringify(starter)
	Player = JSON.stringify(Player)
	money = JSON.stringify(money)
	mon1 = JSON.stringify(mon1)
	mon2 = JSON.stringify(mon2)
	mon3 = JSON.stringify(mon3)
	mon4 = JSON.stringify(mon4)
	mon5 = JSON.stringify(mon5)
	mon6 = JSON.stringify(mon6)
	index = JSON.stringify(index)
	file.store_line(starter)
	file.store_line(Player)
	file.store_line(money)
	file.store_line(mon1)
	file.store_line(mon2)
	file.store_line(mon3)
	file.store_line(mon4)
	file.store_line(mon5)
	file.store_line(mon6)
	file.store_line(index)
	file.close()
